{
	"firebase": "pagepress",
  "public": ".",
	"ignore": [
		"rules.json",
		"firebase-debug.log",
		"**/.*",
		"**/node_modules/**"
	],
	"rewrites": [
		{
			"source": "!/dashboard/*",
			"destination": "/page.html"
		},
		{
			"source": "/dashboard/*",
			"destination": "/dashboard/index.html"
		}
	],
	"rules": "rules.json"
}